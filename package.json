{
	"name": "robotics-club-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"metrics": "node ./metricsLog.js",
		"services": "docker compose up",
		"migrate": "npx prisma migrate dev --name RCCF"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/bcrypt": "^5.0.0",
		"@types/node": "^18.15.3",
		"@types/zxcvbn": "^4.4.1",
		"bcrypt": "^5.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.11.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@picocss/pico": "^1.5.7",
		"@prisma/client": "^4.11.0",
		"pixi.js": "^7.1.4",
		"svelte-device-info": "^1.0.0",
		"zxcvbn": "^4.4.2"
	}
}
